{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/drama/(\\d+)", "dest": "/public/drama-detail.html" },
    { "src": "/(css|js)/(.*)", "dest": "/public/$1/$2" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
